{
  "manifest_version": 3,
  "name": "see on gh",
  "description": "View Sentry issue build on GitHub Actions with one click.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-128.png"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://carepatron.sentry.io/issues/*/*"]
    }
  ]
}
